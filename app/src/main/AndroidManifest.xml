        <activity android:name=".ShowLike" />
        <activity android:name=".UploadPost" />
        <activity android:name=".StaffRegister" />
        <activity android:name=".StudentRegister" />
        <activity android:name=".StudentLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddReport" />
        <activity android:name=".AddMaterial" />
        <activity android:name=".AddNotice" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
