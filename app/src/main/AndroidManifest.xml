        android:theme="@style/AppTheme">
        <activity android:name=".MaterialListActivity"></activity>
        <activity android:name=".ReportListActivity" />
        <activity android:name=".NoticeListActivity" />
        <activity android:name=".StudentListActivity" />
        <activity android:name=".StaffListActivity" />
